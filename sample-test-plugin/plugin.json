{
    "name": "Sample Test Plugin",
    "slug": "sample-test-plugin",
    "version": "1.0.0", 
    "description": "A comprehensive sample plugin demonstrating all Laravel plugin system features",
    "author": "Test Developer",
    "license": "MIT",
    "laravel_version": "^10.0|^11.0",
    "php_version": "^8.1",
    "dependencies": [],
    "requirements": {
        "php": ">=8.1",
        "laravel": ">=10.0"
    },
    "features": [
        "Service Provider Registration",
        "Route Registration (Web & API)",
        "Database Migrations",
        "Blade Views",
        "Asset Management",
        "Configuration",
        "Middleware",
        "Models & Services",
        "Internationalization"
    ],
    "tags": ["test", "sample", "demo", "laravel"],
    "autoload": {
        "psr-4": {
            "Plugins\\SampleTestPlugin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Plugins\\SampleTestPlugin\\Providers\\SampleTestPluginServiceProvider"
            ]
        }
    }
}